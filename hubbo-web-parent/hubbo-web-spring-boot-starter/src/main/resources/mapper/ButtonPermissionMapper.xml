<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.hubbo.web.starter.mapper.ButtonPermissionMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.hubbo.model.pojo.ButtonPermission">
        <id column="id" property="id" />
        <result column="button_permission_code" property="buttonPermissionCode" />
        <result column="button_name" property="buttonName" />
        <result column="enabled" property="enabled" />
        <result column="deleted" property="deleted" />
        <result column="create_by" property="createBy" />
        <result column="create_time" property="createTime" />
        <result column="update_by" property="updateBy" />
        <result column="update_time" property="updateTime" />
        <result column="permission_id" property="permissionId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, button_permission_code, button_name, enabled, deleted, create_by, create_time, update_by, update_time, permission_id
    </sql>

</mapper>
